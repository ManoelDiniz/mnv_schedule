{
  "name": "xsteam-integration",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "dev": "nodemon index.js",
    "build-and-run": "tsc && node dist/src/index.js",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix && prettier -w src/**/*.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/database/dataSource.ts migration:generate ./src/database/migrations/default",
    "migration:run": "[[ \"$NODE_ENV\" = \"production\" ]] && typeorm migration:run -d ./dist/database/dataSource.js || typeorm-ts-node-commonjs -d ./src/database/dataSource.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/database/dataSource.ts",
    "migration:create": "typeorm migration:create ./src/database/migrations/NomedaSuaMigration",
    "seed": "[[ \"$NODE_ENV\" = \"production\" ]] && typeorm-extension seed || ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/dataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@langchain/community": "^0.3.22",
    "@langchain/core": "^0.3.27",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lint": "^0.8.19",
    "lint-staged": "^15.2.11",
    "moment": "^2.30.1",
    "mysql2": "^3.11.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "puppeteer": "^23.11.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
